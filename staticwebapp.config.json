{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/api/*",
      "methods": ["PUT", "POST", "PATCH", "DELETE", "GET", "OPTIONS"]
    }

  ],
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "PUT, DELETE, POST, GET, OPTIONS, PATCH,"
  }
}
